class Book{
     String title,autor;
    int prices,copies;
    int n;
    
    void orderCopies(){
        Scanner sc=new Scanner(System.in);
        System.out.print("enter the number of copies");
        n=sc.nextInt();
        System.out.print(" number of copies are"+n);
    }

  void saleCopy()
    {
        int price=290;
        System.out.print("total price ="+ price*n);
    }
}
