import java.util.InputMismatchException; import java.util.Scanner;
public class Exception {
public static void main(String[] args) { int num1, num2;
Scanner input = new Scanner(System.in);
try
{
System.out.println("Enter num1: "); num1 = input.nextInt(); System.out.println("Enter num2: "); num2 = input.nextInt();
int div = num1 / num2; System.out.println("Division is: "+ div);
}
catch(InputMismatchException e) {
System.out.println("InputMismatchException: Enter integer type input");
}
catch (ArithmeticException e) {
System.out.println("ArithmeticException: Division by zero is not possible ");
}
}
 
 }
